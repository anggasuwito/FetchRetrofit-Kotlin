<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/menuFragment">
    <action
        android:id="@+id/action_to_menuFragment"
        app:destination="@id/menuFragment" />
    <action
        android:id="@+id/action_to_createMenuFragment"
        app:destination="@id/createMenuFragment" />
    <fragment
        android:id="@+id/menuFragment"
        android:name="com.app.fetchretrovit.MenuFragment"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" />
    <fragment
        android:id="@+id/createMenuFragment"
        android:name="com.app.fetchretrovit.CreateMenuFragment"
        android:label="fragment_create_menu"
        tools:layout="@layout/fragment_create_menu" />
</navigation>